# Optional: Additional RPC Providers for redundancy
QUICKNODE_1=https://your-quicknode-endpoint
ALCHEMY_1=https://eth-mainnet.alchemyapi.io/v2/your-api-key
INFURA_1=https://mainnet.infura.io/v3/your-project-id

# Optional: API Keys for external services
TENDERLY_USER=your-tenderly-user
TENDERLY_PROJECT=your-tenderly-project
TENDERLY_ACCESS_KEY=your-tenderly-access-key
TENDERLY_NODE_API_KEY=your-tenderly-node-api-key

# Optional: Gas station info
ETH_GAS_STATION_INFO_URL=https://ethgasstation.info/api/ethgasAPI.json

# Optional: GraphQL endpoint for fee fetching
GQL_URL=https://api.uniswap.org/v1/graphql
GQL_H_ORGN=https://app.uniswap.org

# Optional: Pinata for IPFS
PINATA_API_KEY=your-pinata-api-key
PINATA_API_SECRET=your-pinata-secret

# Optional: Alchemy query keys for subgraphs
ALCHEMY_QUERY_KEY=your-alchemy-query-key
ALCHEMY_QUERY_KEY_2=your-alchemy-query-key-2

# Optional: Debug configuration secret
UNICORN_SECRET=your-debug-secret

# Optional: Internal API key for authenticated requests
INTERNAL_API_KEY=your-internal-api-key

# Performance Configuration
# Increase this for better performance but more memory usage
MAX_ROUTES=3
MAX_SPLIT_HOPS=3
MAX_SWAPS_PER_PATH=3

# Cache Configuration
CACHE_TTL_SECONDS=300
POOL_CACHE_TTL_SECONDS=900
TOKEN_CACHE_TTL_SECONDS=3600

# Logging Configuration
LOG_LEVEL=info
LOG_TO_FILE=false
LOG_FILE_PATH=./logs/routing-api.log 