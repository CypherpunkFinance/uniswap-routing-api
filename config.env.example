# Server Configuration
PORT=3000
DATABASE_PATH=./routing-api.db
NODE_ENV=development

# Required: RPC Provider URLs for supported chains
# Ethereum Mainnet
WEB3_RPC_1=https://eth-mainnet.alchemyapi.io/v2/your-api-key

# Ethereum Sepolia Testnet 
